    "@ledgerhq/hw-transport-u2f": "^4.19.0",
    "@makerdao/dai": "^0.9.6",
    "@makerdao/dai-plugin-governance": "^0.1.0",
    "@makerdao/dai-plugin-ledger-web": "^0.9.3",
    "@makerdao/dai-plugin-trezor-web": "^0.9.2",
    "@makerdao/ui-components": "1.0.0-alpha.8",
    "bignumber.js": "^7.2.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "ethereumjs-tx": "^1.3.6",
    "ethereumjs-util": "^5.2.0",
    "hdkey": "^1.0.0",
    "jazzicon": "^1.5.0",
    "lodash.round": "^4.0.4",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "raven-js": "^3.26.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-ga": "^2.5.3",
    "react-markdown": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^3.4.2",
    "web3-provider-engine": "14.0.4"
  },
  "scripts": {
    "start": "HTTPS=true NODE_ENV=development react-scripts start",
    "build": "react-scripts build",
    "deploy": "now ./build -A ../now.json",
    "test": "jest",
    "cover": "jest --coverage",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "remove-sourcemaps": "rm -rf build/static/js/*.map"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "enzyme": "^3.3.0",
    "ganache-cli": "^6.1.6",
    "husky": "^0.14.3",
    "jest-each": "^23.5.0",
    "jest-fetch-mock": "^1.6.5",
    "lint-staged": "^7.2.0",
    "madge": "^3.3.0",
    "node-fetch": "^2.2.0",
    "prettier": "^1.13.5",
    "pretty": "^2.0.0",
    "redux-mock-store": "^1.5.3",
    "web3-fake-provider": "^0.1.0",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(png|svg|md)$": "<rootDir>/test/helpers/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/helpers/shimRequestAnimationFrame.js",
      "<rootDir>/test/helpers/setup.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
